<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <display-name>oxUma Demo Requesting party</display-name>

    <servlet>
          <servlet-name>rpServiceServlet</servlet-name>
          <servlet-class>org.xdi.uma.demo.rp.server.RpServlet</servlet-class>
          <load-on-startup>1</load-on-startup>
      </servlet>
      <servlet-mapping>
          <servlet-name>rpServiceServlet</servlet-name>
          <url-pattern>/rp/rpService</url-pattern>
      </servlet-mapping>

    <welcome-file-list>
        <welcome-file>rp.html</welcome-file>
    </welcome-file-list>

</web-app>